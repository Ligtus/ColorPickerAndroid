                    android:propertyName="translateY"
                    android:startOffset="110"
                    android:duration="390"
                    android:valueFrom="2"
                    android:valueTo="-10"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
