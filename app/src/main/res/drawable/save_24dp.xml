<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <group android:name="group">
                <path
                    android:name="path_1"
                    android:pathData="M 17 3 L 5 3 C 3.89 3 3 3.9 3 5 L 3 19 C 3 20.1 3.89 21 5 21 L 19 21 C 20.1 21 21 20.1 21 19 L 21 7 L 17 3 Z M 12 19 C 10.34 19 9 17.66 9 16 C 9 14.34 10.34 13 12 13 C 13.66 13 15 14.34 15 16 C 15 17.66 13.66 19 12 19 Z M 15 9 L 5 9 L 5 5 L 15 5 L 15 9 Z"
                    android:fillColor="#000000"
                    android:strokeColor="#000000"
                    android:strokeWidth="0"/>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:duration="50"
                    android:valueFrom="1"
                    android:valueTo="1.3"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:duration="50"
                    android:valueFrom="1"
                    android:valueTo="1.3"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="pivotX"
                    android:duration="300"
                    android:valueFrom="12"
                    android:valueTo="12"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="pivotY"
                    android:duration="300"
                    android:valueFrom="12"
                    android:valueTo="12"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:startOffset="50"
                    android:duration="250"
                    android:valueFrom="1.3"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/bounce_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:startOffset="50"
                    android:duration="250"
                    android:valueFrom="1.3"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/bounce_interpolator"/>
            </set>
        </aapt:attr>
    </target>
</animated-vector>
